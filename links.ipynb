    "def extract_numbers_categories_and_units(text: str) -> list:\n",
    "    \"\"\"\n",
    "    Extract numbers, their associated categories, and units from the given text.\n",
    "\n",
    "    Args:\n",
    "        text (str): The input text to search for numbers, categories, and units.\n",
    "\n",
    "    Returns:\n",
    "        list: A list of dictionaries, each containing 'category', 'number', and 'unit'.\n",
    "    \"\"\"\n",
    "    import re\n",
    "\n",
    "    # Define a regex pattern to match categories, numbers, and optional units\n",
    "    pattern = r'([a-zA-Z\\s]+):?\\s?(\\d{1,3}(?:,\\d{3})*(?:\\.\\d+)?)(?:\\s?([a-zA-Z%]+))?'\n",
    "\n",
    "    # Find all matches in the text\n",
    "    matches = re.findall(pattern, text)\n",
    "\n",
    "    # Create a list to store the results\n",
    "    extracted_data = []\n",
    "    for category, number, unit in matches:\n",
    "        # Clean up the category, number, and unit\n",
    "        category = category.strip().lower()\n",
    "        number = number.replace(\",\", \"\")  # Remove commas from numbers\n",
    "        unit = unit.strip().lower() if unit else None\n",
    "\n",
    "        extracted_data.append({\n",
    "            \"category\": category,\n",
    "            \"number\": float(number) if '.' in number else int(number),\n",
    "            \"unit\": unit\n",
    "        })\n",
    "\n",
    "    return extracted_data\n",
    "\n",
    "extract_numbers_categories_and_units(tfc)"
   ]
