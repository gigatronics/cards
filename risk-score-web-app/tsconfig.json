{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "frontend/src"
  },
  "include": ["frontend/src/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}